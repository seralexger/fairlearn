    "# show_proportions is only a helper function for plotting\n",
    "def show_proportions(X, sensitive_features, y_pred, y=None, description=None, plot_row_index=1):\n",
    "    print(\"\\n\" + description)\n",
    "    \n",
    "    axis_label = 'recidivism'\n",
